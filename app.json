{
  "expo": {
    "name": "Sush",
    "slug": "mainapp",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/iconround.png",
    "scheme": "mainapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash2.png",
      "resizeMode": "cover",
      "backgroundColor": "#C9D6FF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sush.mainapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/iconround.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sush.mainapp"
    },
    "web": {
      "favicon": "./assets/images/iconround.png"
    }
   
  }
}
